{
  "name": "samueleresca-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "get-database": "curl https://raw.githubusercontent.com/samueleresca/samuelerescablog-dev-db/master/ghost.db -o ghost.db",
    "watch": "npx mix watch",
    "hot": "npx mix watch --hot",
    "docker-watch": "concurrently --names 'DOCKER,WEBPACK' --prefix-colors 'yellow,magenta' --kill-others \"docker-compose up\" \"npm run watch\"",
    "podman-watch": "podman-compose up -d && npm run watch",
    "deploy": "npm run production && node deploy/index.js",
    "production": "npm run build && npm run zip",
    "build": "npx mix --production",
    "zip": "run-script-os",
    "zip:win32": "cd .. && 7z a -r -x!.git -x!src -x!samueleresca-ui.zip samueleresca-ui.zip *",
    "zip:default": "cd .. && zip -r samueleresca-ui.zip ./* -x '*.git*' -x '*src*' -x 'samueleresca-ui.zip'",
    "order-locales": "node order-locales.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@tailwindcss/typography": "^0.5.9",
    "@tryghost/admin-api": "^1.4.0",
    "7zip": "0.0.6",
    "@tryghost/admin-api": "^1.4.2",
    "browser-sync": "^2.27.12",
    "browser-sync-webpack-plugin": "^2.3.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.0",
    "fs": "0.0.1-security",
    "ghost-cli": "^0.0.2",
    "laravel-mix": "^6.0.49",
    "mix-tailwindcss": "^1.3.0",
    "path": "^0.12.7",
    "postcss": "^8.4.14",
    "prettier": "2.8.4",
    "resolve-url-loader": "5.0.0",
    "run-script-os": "^1.1.5",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "@tryghost/content-api": "^1.11.1",
    "cookieconsent": "^3.1.1"
  },
  "optionalDependencies": {
    "win-node-env": "^0.6.0"
  }
}
